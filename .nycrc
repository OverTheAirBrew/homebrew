{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": [
    "dist/src/**/*",
    "src/**/*"
  ],
  "exclude": [
    "**/*.spec.js",
    "**/*.d.ts",
    "**/orm/**/*",
    "**/workers/**/*",
    "**/hooks/**/*",
    "**/container.ts",
    "src/app/index.ts",
    "**/controllers/index.ts",
    "**/hooks/index.ts",
    "**/workers/index.ts",
    "**/lib/socket-io.ts"
  ],
  "sourcemap": false,
  "branches": 100,
  "functions": 100,
  "lines": 100,
  "statements": 100
}
