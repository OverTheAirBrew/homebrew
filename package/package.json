    "@types/node": "^15.12.2",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/umzug": "^2.3.0",
    "@types/validator": "^13.1.4",
    "apidocs": "^2017.3.9",
    "chai": "^4.3.4",
    "codecov": "^3.8.2",
    "del-cli": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "sinon": "^11.1.1",
    "supertest": "^6.1.3",
    "swagger-node-codegen": "^1.6.3",
