{
  "name": "homebrew",
  "version": "0.2.3",
  "main": "app/index.js",
  "scripts": {
    "test": "nyc --reporter=text-summary --reporter=lcov mocha 'test/**/*.spec.js' --exit",
    "coverage": "codecov",
    "prebuild": "del '**/*.{js,map,d.ts}' '!node_modules'",
    "build": "tsc -p ./tsconfig.json",
    "watch": "tsc --watch",
    "run-dev": "nodemon ./app/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "git pull origin master",
    "generatedocs": "snc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/express": "^4.17.12",
