{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "jsdom",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage-e2e",
  "collectCoverageFrom": [
    "src/app/*.(t|j)s",
    "cron/app/*.(t|j)s",
    "workers/app/*.(t|j)s"
  ]
}
